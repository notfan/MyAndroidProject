<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wor.smartdrugbox">

    <application
        android:name=".app.SmartDrugBoxApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.ReportActivity"
            android:label="@string/title_activity_report"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.AppointmentActivity"
            android:label="@string/title_activity_appointment"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.NoteActivity"
            android:label="@string/title_activity_note"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.DoctorActivity"
            android:label="@string/title_activity_doctor"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.ContactActivity"
            android:label="@string/title_activity_contact"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.TipActivity"
            android:label="@string/title_activity_tip"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.SettingActivity"
            android:label="@string/title_activity_setting"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.HelpActivity"
            android:label="@string/title_activity_help"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.WeekViewActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.CellDetailActivity"
            android:label="@string/title_activity_cell_detail"
            android:parentActivityName=".ui.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.wor.smartdrugbox.ui.MainActivity" />
        </activity>
        <activity
            android:name=".ui.DrugSettingActivity"
            android:label="@string/title_activity_drug_setting"
            android:parentActivityName=".ui.CellDetailActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.wor.smartdrugbox.ui.CellDetailActivity" />
        </activity>
        <activity
            android:name=".ui.CellSettingActivity"
            android:label="@string/title_activity_cell_setting"
            android:parentActivityName=".ui.CellDetailActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.wor.smartdrugbox.ui.CellDetailActivity" />
        </activity>
        <activity
            android:name=".ui.AppointmentDetailActivity"
            android:label="@string/title_activity_appointment_detail"
            android:parentActivityName=".ui.AppointmentActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.wor.smartdrugbox.ui.AppointmentActivity" />
        </activity>
        <activity
            android:name=".ui.AppointmentSettingActivity"
            android:label="@string/title_activity_appointment_setting"
            android:parentActivityName=".ui.AppointmentActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.wor.smartdrugbox.ui.AppointmentActivity" />
        </activity>
        <activity
            android:name=".ui.NoteDetailActivity"
            android:label="@string/title_activity_note_detail"
            android:parentActivityName=".ui.NoteActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.wor.smartdrugbox.ui.NoteActivity" />
        </activity>
        <activity
            android:name=".ui.NoteSettingActivity"
            android:label="@string/title_activity_note_setting"
            android:parentActivityName=".ui.NoteActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.wor.smartdrugbox.ui.NoteActivity" />
        </activity>
        <activity
            android:name=".ui.DoctorSettingActivity"
            android:label="@string/title_activity_doctor_setting"
            android:parentActivityName=".ui.DoctorActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.wor.smartdrugbox.ui.DoctorActivity" />
        </activity>
        <activity
            android:name=".ui.ContactDetailActivity"
            android:label="@string/title_activity_contact_detail"
            android:parentActivityName=".ui.ContactActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.wor.smartdrugbox.ui.ContactActivity" />
        </activity>
        <activity
            android:name=".ui.ContactSettingActivity"
            android:label="@string/title_activity_contact_setting"
            android:parentActivityName=".ui.ContactActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.wor.smartdrugbox.ui.ContactActivity" />
        </activity>
        <activity
            android:name=".ui.DoctorDetailActivity"
            android:label="@string/title_activity_doctor_detail"
            android:parentActivityName=".ui.DoctorActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.wor.smartdrugbox.ui.DoctorActivity" />
        </activity>
    </application>

</manifest>